<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
  version="2.0" xmlns="http://java.sun.com/xml/ns/persistence">
  <persistence-unit name="feidroid" transaction-type="RESOURCE_LOCAL">
   <class>sk.stuba.fei.feidroid.entities.Application</class>
    <class>sk.stuba.fei.feidroid.entities.ApplicationCategory</class>
    <class>sk.stuba.fei.feidroid.entities.Permission</class>
    <class>sk.stuba.fei.feidroid.entities.PermissionUsage</class>
    <class>sk.stuba.fei.feidroid.entities.PermissionAnalysis</class>
    <class>sk.stuba.fei.feidroid.appconfig.entities.AppConfig</class>
    <class>sk.stuba.fei.feidroid.entities.PermissionUsageRelation</class>
    <properties>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver" />
      <property name="javax.persistence.jdbc.url"
        value="jdbc:mysql://localhost:3306/feidroid" />
      <property name="javax.persistence.jdbc.user" value="root" />
      <property name="javax.persistence.jdbc.password" value="" />
    </properties>

  </persistence-unit>
</persistence> 